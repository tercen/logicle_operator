{
  "name": "Logicle",
  "description": "Performs a channel-specific logicle data tranformation.",
  "tags": [
    "flow cytometry",
    "preprocessing",
    "transformation"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/logicle_operator"
  ],
  "properties": [],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
          "kind": "CrosstabSpec",
          "metaFactors": [
              {
                  "kind": "MetaFactor",
                  "name": "Channel",
                  "type": "",
                  "description": "The channel identifiers",
                  "ontologyMapping": "variable",
                  "crosstabMapping": "column",
                  "cardinality": "1..n",
                  "factors": []
              },
              {
                  "kind": "MetaFactor",
                  "name": "events",
                  "type": "",
                  "description": "the observations",
                  "ontologyMapping": "observation",
                  "crosstabMapping": "row",
                  "cardinality": "1..n",
                  "factors": []
              }
          ],
          "axis": [
              {
                  "kind": "AxisSpec",
                  "metaFactors": [
                      {
                          "kind": "MetaFactor",
                          "name": "data",
                          "type": "numeric",
                          "description": "the measurements",
                          "ontologyMapping": "measurement",
                          "crosstabMapping": "y",
                          "cardinality": "1",
                          "factors": []
                      }
                  ]
              }
          ]
      }
  ]
  }
}